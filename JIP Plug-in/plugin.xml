<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="net.sf.jiprof.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="JIP Perspective"
            class="net.sf.jiprof.Perspective"
            id="net.sf.jiprof.perspective">
      </perspective>
   </extension>

</plugin>
