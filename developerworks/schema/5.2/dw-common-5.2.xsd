<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:dw="http://www.ibm.com/developerWorks/" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="5.1">
  <!-- ================================================================ -->
  <!-- <a> (anchor) element   -->
  <!-- ================================================================ -->
  <xsd:element name="a">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a hypertext anchor</title>
        <desc>Defines a hypertext link.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="i"/>
        <xsd:element ref="img"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
      <xsd:attribute name="href" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the target URL of this hypertext link. </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="name" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Gives this location a name.  Named anchors can be referenced just like external URLs.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="onClick" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the action that should take place when the user clicks this link.  This is typically JavaScript code or a call to a JavaScript function. </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="target" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines a target for this link.  You can use this attribute to refer to a named browser window.  A value of <code>_blank</code> means the linked document will appear in a new browser window.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <!-- 4.0 6/1 tdc:  Removed the trackclick attribute per editors -->
    </xsd:complexType>
    <!-- Defined as ANY in the DTD -->
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <abstract> element                                               -->
  <!-- ================================================================ -->
  <xsd:element name="abstract" type="xsd:string">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>abstract</title>
        <desc>Brief overview of the document that will be displayed if no abstract-extended element is present, as well as in content views and search results; should contain information to help the user decide whether to read the document.  Used to create the meta abstract tag, which contributes to relevance in the ibm.com FAST search. Also used for e-mail to a friend.  No HTML coding allowed since this is used for the meta tags.  Use these guidelines to create: http://dw.raleigh.ibm.com/dwteam/search/search2003/guidelines/meta-guidelines.doc</desc>
      </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <abstract-extended> element                                               -->
  <!-- ================================================================ -->
  <xsd:element name="abstract-extended">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>A long summary of the article</title>
        <desc>Optional, longer abstract that can be used as a page element of the document.  Use if you want a longer abstract or need to use HTML coding in the abstract that is a page element in the document.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="i"/>
        <xsd:element ref="p"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <attribution> element                                            -->
  <!-- ================================================================ -->
  <xsd:element name="attribution">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define the external source of an article</title>
        <desc>Credit line if source of the article is external, like an external magazine (JavaWorld), or Web site.</desc>
        <note>Use this value to create the DC.Publisher meta tag according to ibm.com meta tag standards.  If this value is not specified with a third party name, then fill in "IBM Corporation"</note>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:group ref="common-doc-group"/>
        <!-- <xsd:element ref="common-doc"/> -->
      </xsd:choice>
      <xsd:attribute name="type" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The source of the article.  </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <!-- 12/1/03 TDC:  Removed default='javaworld'.  Should probably use an enumeration here in 2.2+ to trigger different attribution statements in xsl. -->
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <audience> element                                            -->
  <!-- ================================================================ -->
  <xsd:element name="audience">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>The typical user of this content</title>
        <desc>Describes the typical user that might be interested in this document.  Do not list skills in this section</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:group ref="common-doc-group"/>
        <!-- <xsd:element ref="common-doc"/> -->
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <author> element        														-->
  <!-- ================================================================ -->
  <xsd:element name="author">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define information about the author of the article</title>
        <desc>Can have multiple authors.  Future (post-3.0):  Authors can have multiple bios.  Currently "name" is used for author name in schema until investigation is complete on how to handle parts of name (prefix, firstname, middlename, lastname, suffix).</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="bio"/>
        <xsd:element ref="img" minOccurs="0"/>
        <xsd:choice>
          <xsd:element ref="name" minOccurs="0"/>
          <xsd:element name="author-name" type="dw-person-name" minOccurs="0"/>
        </xsd:choice>
        <xsd:element ref="company-name"/>
      </xsd:sequence>
      <xsd:attribute name="email" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The author's email address.  Need some method so that author's e-mail address isn't visible on the page.  Once this requirement is met, should be Required.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="jobtitle" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The author's job title.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="email-cc" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Allows someone on the dW team, like an acquisition editor, to be copied on author e-mails.  Need some method so that cc e-mail address isn't visible on the page.  Once this requirement is met, should be Required.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <!-- 4.0 7/23 tdc:  Rename email_cc to email-cc -->
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <author-summary> element        -->
  <!-- ================================================================ -->
  <xsd:element name="author-summary" type="contributor">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define information about the author of the summary</title>
        <desc>Authors can have multiple bios.</desc>
      </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <!-- 5.0 3/16 tdc:  Changed name from author-tutorial to author-tutorial-summary -->
  <!-- ================================================================ -->
  <!-- <author-tutorial-summary> element  													-->
  <!-- ================================================================ -->
  <xsd:element name="author-tutorial-summary">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define information about contributor</title>
        <desc>A contributor to any content.  Describes author, editor, etc.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="contributor-name" type="dw-person-name"/>
        <xsd:element ref="company-name" minOccurs="0"/>
      </xsd:sequence>
      <xsd:attribute name="email" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The email address. </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="jobtitle" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The contributor's job title.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="email-cc" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Allows someone on the dW team, like an acquisition editor, to be copied on contributor e-mails.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <b> (bold) element                                               -->
  <!-- ================================================================ -->
  <xsd:element name="b">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define boldfaced text</title>
        <desc>Defines boldfaced text. </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="font"/>
        <xsd:element ref="i"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
        <!-- 4/7/03 Doug added <font> temporarily -->
      </xsd:choice>
      <xsd:attribute name="class" type="colorname">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the color scheme used by this element's text.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <bio> element                                                    -->
  <!-- ================================================================ -->
  <xsd:element name="bio">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <xsd:minLength value="10"/>
        <title>A short biography of the author</title>
        <desc>Defines a short biography of the author.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="i"/>
        <xsd:element ref="p"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <blockquote> element                                             -->
  <!-- ================================================================ -->
  <xsd:element name="blockquote">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a lengthy quote</title>
        <desc>Defines a long quote.  The text is indented on the left and right margins.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="i"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
        <!-- 5.2 8/31/05 fjc:  Added ul and ol for V. Suddaby -->
        <xsd:element ref="ul"/>
        <xsd:element ref="ol"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <br> (break) element                                             -->
  <!-- ================================================================ -->
  <xsd:element name="br">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a line break</title>
        <desc>Defines a line break in the text. </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="empty"/>
    </xsd:simpleType>
  </xsd:element>
  <!-- 4.0 5/20 tdc:  removed breadcrumbappend element definition -->
  <!-- 5.2 8/22/05 tdc:  Removed <bundle-component> element -->
  <!-- 5.2 8/22/05 tdc:  Removed <bundle-components> element  -->
  <!-- 5.0 6/21 tdc:  Added caption -->
  <!-- ================================================================ -->
  <!-- <caption> element                                                   -->
  <!-- ================================================================ -->
  <xsd:element name="caption">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Caption</title>
        <desc>Used to describe table contents.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="em" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <code> element                                                   -->
  <!-- ================================================================ -->
  <xsd:element name="code">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a code listing</title>
        <desc>The stylesheet allows code to be inline or section.  The contents of this element are displayed in a monospaced font, with all whitespace preserved from the original XML source.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="font"/>
        <xsd:element ref="heading"/>
        <xsd:element ref="i"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
      <xsd:attribute name="type" type="inline" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The type of code listing.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="width">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The width in characters of this code listing.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <column-info> element                                            -->
  <!-- ================================================================ -->
  <xsd:element name="column-info">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define meta information for this column</title>
        <desc>Defines meta information for this column, such as an icon or a title.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="col-icon" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines an icon for this column.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="col-name" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines a name for this column.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <!-- ** This is defined as ANY in the DTD, but it looks like it's empty. -->
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <company-name> element    -->
  <!-- ================================================================ -->
  <xsd:element name="company-name">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Company name</title>
        <desc>Defines the name of a company. </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:group ref="specialCharacters"/>
        <!-- 4.0 12/09/04 fjc: we may want to change this to a URL element -->
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- 4.0 4/15 tdc: replaced content-area with content-area-primary and content-area-secondary. -->
  <!-- ================================================================ -->
  <!-- <content-area-primary> element                                                   -->
  <!-- ================================================================ -->
  <xsd:element name="content-area-primary">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define the primary content area for this article</title>
        <desc>Indicates the primary dW product and/or technology content area that should contain this document.   Used to create page elements like breadcrumb trail. Search:  Request meta tag. </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="name" type="content-area" use="required"/>
    </xsd:complexType>
  </xsd:element>
  <!-- 4.0 4/15 tdc: replaced content-area with content-area-primary and content-area-secondary. -->
  <!-- ================================================================ -->
  <!-- <content-area-secondary> element                                                   -->
  <!-- ================================================================ -->
  <xsd:element name="content-area-secondary">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define the secondary content area for this article</title>
        <desc>Indicates a secondary dW product and/or technology content area that should contain this document.  Request meta tag. </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="name" type="content-area" use="required"/>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <copyright> element                                                    -->
  <!-- ================================================================ -->
  <xsd:element name="copyright">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>A document copyright</title>
        <desc>Copyright of the spec.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:group ref="common-doc-group"/>
        <!-- <xsd:element ref="common-doc"/> -->
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <course-number> element                                                    -->
  <!-- ================================================================ -->
  <xsd:element name="course-number">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>A Course Number</title>
        <desc>Number the of the course.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:group ref="common-doc-group"/>
        <!-- <xsd:element ref="common-doc"/> -->
      </xsd:choice>
      <xsd:attribute name="display-in-title-area" type="yesno" use="optional"/>
    </xsd:complexType>
  </xsd:element>
  <!-- 5.0 6/21 tdc:  Added emphasis (em) -->
  <!-- 4.0 4/19 tdc: renamed date to date-published -->
  <!-- ================================================================ -->
  <!-- <date-published> element                                                   -->
  <!-- ================================================================ -->
  <xsd:element name="date-published">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>The date this article was published.</title>
        <desc>Date published on the supersite or local site.</desc>
        <note>Automatically update the DC.Date and IBM.Effective meta tags with this date - phase 2 of schema/stylesheet.  Date must be conform to ibm.com meta tag standards.</note>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attributeGroup ref="dmy"/>
    </xsd:complexType>
    <!-- ** Should be an XML Schema date type. -->
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <date-updated> element                                           -->
  <!-- ================================================================ -->
  <xsd:element name="date-updated">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Date the document's *content* is updated.</title>
        <desc>Date the document's *content* is updated.  Not a system date, and does not include updates for typos, etc.  Stylesheet automatically updates the d.c. date meta tag with this date, if present.  Date must be presented in international format.  Internally can have unbounded number of updates but UI should only show last update.</desc>
        <note>Automatically update the DC.Date and IBM.Effective meta tags with this date - phase 2 of schema/stylesheet.  Date must be conform to ibm.com meta tag standards.    Internally can have unbounded number of updates but UI should only show last update.</note>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attributeGroup ref="dmy"/>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <dd> (definition) element                                        -->
  <!-- ================================================================ -->
  <xsd:element name="dd">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>The definition of a term</title>
        <desc>This element contains the definition of a term in a definition list.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="dl"/>
        <xsd:element ref="i"/>
        <xsd:element ref="ol"/>
        <xsd:element ref="p"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:element ref="ul"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <dl> (definition list) element                                   -->
  <!-- ================================================================ -->
  <xsd:element name="dl">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>A definition list</title>
        <desc>Defines a definition list.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence maxOccurs="unbounded">
        <xsd:element ref="dt"/>
        <xsd:element ref="dd"/>
      </xsd:sequence>
      <xsd:attribute name="compact" type="yesno">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines whether or not this definition list should be compacted.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <docbody> element                                                -->
  <!-- ================================================================ -->
  <!-- ** Do we want this to contain text?  I don't think so... -->
  <xsd:element name="docbody">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define the content of a document</title>
        <desc>Defines the content of a document.  This element can contain text, along with the various elements that define the basic layout of text elements.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:group ref="common-doc-group"/>
        <!-- <xsd:element ref="common-doc"/> -->
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- 4.0 5/10 tdc:  use target-content instead of download-feature -->
  <!-- ================================================================ -->
  <!-- <dt> element                                                     -->
  <!-- ================================================================ -->
  <xsd:element name="dt">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a term</title>
        <desc>Defines a term in a definition list.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="i"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <duration> element                                            -->
  <!-- ================================================================ -->
  <xsd:element name="duration">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Time needed to complete the tasks in the content module</title>
        <desc>Time needed to complete the tasks in the content module</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:group ref="common-doc-group"/>
        <!-- <xsd:element ref="common-doc"/> -->
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <em> element                                                   -->
  <!-- ================================================================ -->
  <xsd:element name="em">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Emphasis</title>
        <desc>Used to apply emphasis to text.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <enrollment-schedule> element                                            -->
  <!-- ================================================================ -->
  <xsd:element name="enrollment-schedule">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define the enrollment-schedule provider</title>
        <desc>The enrollment-schedule provider  makes up the View schedules and enroll section--all providers listed</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:sequence>
        <xsd:element name="provider" type="xsd:string" maxOccurs="unbounded"/>
        <xsd:element ref="course-number"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <figure> element                                                 -->
  <!-- ================================================================ -->
  <xsd:element name="figure">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a illustrative detail</title>
        <desc>Define a code sample, a diagram, or some other illustrative detail.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="heading"/>
        <xsd:element ref="img" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <!-- 5.0 04/29 tdc:  file-format (singular) (was named file-formats)  -->
  <!-- ================================================================ -->
  <!-- <file-format> element                                           								 -->
  <!-- ================================================================ -->
  <xsd:element name="file-format" type="file-type">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>File format</title>
        <desc>The format (file type) that a given file is available in.</desc>
      </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <!-- 5.0 04/29 tdc:  Added file-formats to contain one or more file-format elements -->
  <!-- ================================================================ -->
  <!-- <file-formats> element                                               -->
  <!-- ================================================================ -->
  <xsd:element name="file-formats">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>File formats</title>
        <desc>One or more file formats that a given file is available in.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="false">
      <xsd:choice maxOccurs="unbounded">
        <xsd:element ref="file-format"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <font> element                                                   -->
  <!-- ================================================================ -->
  <xsd:element name="font">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <desc>Defines the font to be used for this content. </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="dl"/>
        <xsd:element ref="font"/>
        <xsd:element ref="i"/>
        <xsd:element ref="ol"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:element ref="table"/>
        <xsd:element ref="ul"/>
        <xsd:group ref="specialCharacters"/>
        <!-- <font> is evil, must be killed -->
      </xsd:choice>
      <xsd:attribute name="color" type="xslfoColor">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The color for the font.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="face" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The face name used for this font.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="size">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The size of the font. </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <foreign-language> element                                                    -->
  <!-- ================================================================ -->
  <xsd:element name="foreign-language">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>This document is available in another language</title>
        <desc>The languages this document is available in</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="language" type="xsd:string"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- 4.0 4/19 tdc: renamed forumurl to forum-url -->
  <!-- ================================================================ -->
  <!-- <forum-url> element                                               -->
  <!-- ================================================================ -->
  <xsd:element name="forum-url">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Associate a forum with this article</title>
        <desc>A document-specific forum link (discuss it icon)</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="url" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The URL of the associated forum.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <heading> element                                                                                                   -->
  <!-- ================================================================ -->
  <xsd:element name="heading">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a heading</title>
        <desc>Defines the text of a heading.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="i"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
      <xsd:attribute name="alttoc" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines alternate text to be used in the table of contents.  This attribute is useful when the heading has lots of text. </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="name" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Names this heading.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="refname" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>????Defines an ID for this heading.  You can use this attribute to reference this heading from elsewhere in the document.???? </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="type" type="headingtype" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the type of heading.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <!-- 5.0 5/26 tdc:  Added toc back for M. Chan to ease CMA migration of old content only.  CMA team to remove toc attribute from all XML files after OneX deadline met.  -->
      <xsd:attribute name="toc" type="yesno" use="optional">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Deprecated in V5 - only here to ease CMA migration of pre-V5 content.  CMA team to remove this attribute from all XML files after OneX deadline.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <!-- 5.2 8/25/05 tdc:  Added back-to-top -->
      <xsd:attribute name="back-to-top" type="yesno" use="optional">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Indicates whether the stylesheet should add a Back to top link before the next major heading.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <i> (italic) element                                             -->
  <!-- ================================================================ -->
  <xsd:element name="i">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define italicized text</title>
        <desc>Defines text that will appear in an italic font.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <id> element                                                     -->
  <!-- lk added content-id as string to support wsdd and db2 content ratings data -->
  <!-- ================================================================ -->
  <xsd:element name="id">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define an ID for a document</title>
        <desc>A unique identification number for each piece of content that is generated by the content management team following a standard convention.</desc>
        <note>Future (post-4.0):  Make id an attrib of dw-document?  We'll only need one format across all document types, not multiple formats as currently defined.</note>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="domino-uid" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the unique Domino document ID for this document.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="cma-id" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the unique content id used by the dW content management application.  This value is not used by ratings systems yet.  Once we're all using a common ratings system, this ID will be the only one we need and we can get rid of content-id and domino-uid.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="content-id" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the unique content id (non-Domino) for this document.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="original" type="yesno" default="yes">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Specifies whether the content associated with this ID is the original, or a translated version.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <!-- 4.0 7/1 tdc:  Added cma-id -->
      <!-- 4.0 4/29 tdc: Added to enable mapping of translated and original content -->
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <img> element                                                    -->
  <!-- ================================================================ -->
  <xsd:element name="img">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define an image</title>
        <desc>Defines an image that will appear in the article. </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="align" type="alignment">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines how the image should be aligned.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="alt" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines alternate text for the graphic.  This text appears in non-graphical browsers and also may be used by accessibility aids such as screen readers.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="border" type="zeroOrOne">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines whether this image will have a border.  </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="height" type="xsd:integer">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the height in pixels of the image.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="hspace" type="xsd:integer">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the horizontal space around the image.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="name" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines a name for the image.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="src" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the location of the image.  This can be a relative URL or an absolute URL.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="valign" type="vertical">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the vertical alignment of the image.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="vspace" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the vertical space around the image.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="width">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the width in pixels of the image.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <!-- 4.0 05/24 tdc:  alt tag now required; conversion stylesheet will add one if not present already -->
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <instructor> element        -->
  <!-- ================================================================ -->
  <xsd:element name="instructor" type="contributor">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define information about the instructor of a course or class</title>
        <desc>Instructor</desc>
      </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <in-this-summary> element                                           						 -->
  <!-- ================================================================ -->
  <xsd:element name="in-this-doc">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>In This doc</title>
        <desc>Bulleted list of major headings.  doc is replaced by the content type; for example, "In this tutorial"</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice maxOccurs="unbounded">
        <xsd:group ref="common-doc-group"/>
        <!-- <xsd:element ref="common-doc"/> -->
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- 4.0 4/22 tdc: added keywords for meta keywords (which are required, and other meta tags are not). -->
  <!-- ================================================================ -->
  <!-- <keywords> element (for meta keywords)                                       -->
  <!-- ================================================================ -->
  <xsd:element name="keywords">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define keywords for this article.</title>
        <desc>Used to create META keywords.  Used by FAST and other search engines.  Use these guidelines to create keywords:  http://dw.raleigh.ibm.com/dwteam/search/search2003/guidelines/meta-guidelines.html</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="content" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The keywords, each separated by a comma.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <li> element                                                     -->
  <!-- ================================================================ -->
  <xsd:element name="li">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define an item in a list</title>
        <desc>Defines a list item for an ordered or unordered list.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="dl"/>
        <xsd:element ref="figure"/>
        <xsd:element ref="font"/>
        <xsd:element ref="i"/>
        <xsd:element ref="img"/>
        <xsd:element ref="ol"/>
        <xsd:element ref="p"/>
        <xsd:element ref="pre"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:element ref="table"/>
        <xsd:element ref="ul"/>
        <xsd:group ref="specialCharacters"/>
        <!-- 4.0 4/26 tdc:  added font element per request #126 -->
      </xsd:choice>
      <xsd:attribute name="value" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>??? Is this the same as the HTML value attribute?  If so, this changes the item number for this item.  Subsequent list items are numbered accordingly as well. ???</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <link-to-english> element                                           							 -->
  <!-- ================================================================ -->
  <xsd:element name="link-to-english">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Link to English summary page.</title>
        <desc>For local site content, link to English summary page</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice maxOccurs="unbounded">
        <xsd:element ref="a"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- 4.0 4/30 tdc:  removed meta element per design req. #206 -->
  <!-- 4.0 4/30 tdc:  added meta-last-updated element for last update meta tag per design req. #206 -->
  <!-- ================================================================ -->
  <!-- <meta-last-updated> element                                        -->
  <!-- ================================================================ -->
  <xsd:element name="meta-last-updated">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Meta last updated</title>
        <desc>Defines the HTML META tag that indicates when an editor last changed this file.  Not to be confused with date-updated, which contains the date when content was significantly updated and republished.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attributeGroup ref="dmy">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The day, month, and year (with leading zeros) an editor last changed this file.  Not to be confused with date-updated, which contains the date when content was significantly updated and republished.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attributeGroup>
      <xsd:attribute name="initials" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The initials (preferably at least three letters) of the editor who last changed this file.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <name> element    -->
  <!-- ================================================================ -->
  <xsd:element name="name" type="name-type">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Author name</title>
        <desc>Defines the name of an individual author.  Will remain until investigation and resolution of how to handle the different parts of author name (prefix, firstname, middlename, lastname, suffix)</desc>
      </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <!-- The objectives of the content.  -->
  <!-- ================================================================ 	-->
  <!-- <objectives> element                                                     							-->
  <!-- ================================================================	-->
  <xsd:element name="objectives">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>The objectives of the content</title>
        <desc>Gives the overall learning goals of the tutorial or online course. Could also be used in how-to documents or other doctypes.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="objective" type="xsd:string"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <ol> (ordered list) element                                      -->
  <!-- ================================================================ -->
  <xsd:element name="ol">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define an ordered list</title>
        <desc>Defines an ordered list.  It contains some number of &lt;li&gt; items.  You can also nest ordered and unordered lists inside the list.</desc>
        <!--, which can in turn contain other lists, such as ordered lists, unordered lists, or definition lists.  In other words, an unordered list can't be a child of an ordered list; you must put the unordered list (at the second level) inside a list item.</desc> -->
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:choice maxOccurs="unbounded">
        <xsd:element ref="li"/>
        <xsd:element ref="ol"/>
        <xsd:element ref="ul"/>
      </xsd:choice>
      <xsd:attribute name="start" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the starting number for this list.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="type" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the type of numbering that should be used for this list.  Valid values are:</desc>
            <ul>
              <li>
                <code>1</code> - Items are numbered with Arabic numerals (<code>1</code>, <code>2</code>, <code>3</code>, <code>4</code>, <code>5</code>, and so forth). This is the default numbering style.</li>
              <li>
                <code>A</code> - Items are numbered with uppercase alpha characters (<code>A</code>, <code>B</code>, <code>C</code>, <code>...</code>, <code>AA</code>, <code>AB</code>, <code>AC</code>, and so forth)</li>
              <li>
                <code>a</code> - Items are numbered with lowercase alpha characters (<code>a</code>, <code>b</code>, <code>c</code>, <code>...</code>, <code>aa</code>, <code>ab</code>, <code>ac</code>, and so forth)</li>
              <li>
                <code>I</code> - Items are numbered with uppercase Roman numerals (<code>I</code>, <code>II</code>, <code>III</code>, <code>IV</code>, <code>V</code>, and so forth)</li>
              <li>
                <code>i</code> - Items are numbered with lowercase Roman numerals (<code>i</code>, <code>ii</code>, <code>iii</code>, <code>iv</code>, <code>v</code>, and so forth)</li>
            </ul>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <overview> element                                            -->
  <!-- ================================================================ -->
  <xsd:element name="overview">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>General intro text required in addition to abstract or abstract extended</title>
        <desc>General intro text required in addition to abstract or abstract extended</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:group ref="common-doc-group"/>
        <!-- <xsd:element ref="common-doc"/> -->
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <p> (paragraph) element                                          -->
  <!-- ================================================================ -->
  <xsd:element name="p">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a paragraph</title>
        <desc>Defines a paragraph of text.  </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="dl"/>
        <xsd:element ref="font"/>
        <xsd:element ref="i"/>
        <xsd:element ref="img"/>
        <xsd:element ref="ol"/>
        <xsd:element ref="sidebar"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:element ref="table"/>
        <xsd:element ref="ul"/>
        <xsd:group ref="specialCharacters"/>
        <!-- <font> is evil, must be killed -->
      </xsd:choice>
      <xsd:attribute name="align" type="alignment">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the alignment for this paragraph.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <pdf> element                                                    -->
  <!-- ================================================================ -->
  <xsd:element name="pdf">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a link to a PDF file</title>
        <desc>Link to printer-friendly PDF version of the document.</desc>
        <note>Only required when we have a mechanism to automatically create a PDF. Right now, some DD content has a printer-friendly version, though it is not a PDF; objective is for PDF to be used as the printer-friendly version for all documents.  PDF must be A4 and letter.  Will continue to do printer friendly for domains using it.  If non-open standards printable doc, put in the Resources section.  Who has other non-open types and what are they?</note>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="paperSize" type="paperSize">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The paper size used in the PDF file. </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="size" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The size in KB of the PDF file.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="url" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The URL of the PDF file.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <!-- 5.2 8/17/05 tdc:  Added pages attribute -->
      <xsd:attribute name="pages" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The number of pages in the PDF file.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <pre> element                                                    -->
  <!-- ================================================================ -->
  <xsd:element name="pre">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define preformatted content</title>
        <desc>Defines preformatted content.  All white space characters (spaces, newlines, tabs, and so forth) are preserved.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="i"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- 10/17/05 FJC added for the spec summary doc-->
  <!-- ================================================================ -->
  <!-- <prerequisites> element                                            -->
  <!-- ================================================================ -->
  <xsd:element name="prerequisites">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Lists the skiils that readers need prior to reading this content.</title>
        <desc>Lists the skiils that readers need prior to reading this content.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:sequence>
        <xsd:group ref="common-doc-group"/>
      </xsd:sequence>
    </xsd:complexType>
    <!-- 5.0 3/11 tdc: common-doc-group instead of element called common-doc -->
    <!--
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="common-doc"/>
			</xsd:choice>
		</xsd:complexType>
		-->
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <registration> element                                            -->
  <!-- ================================================================ 
	<xsd:element name="registration">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>This content can only be viewed by registered users</title>
				<desc>After you register, you can use your userid and password to access any tutorial on the site without entering them again. Be sure to remember your userid and password. Whenever you restart your browser, you'll need to re-enter them to access any tutorial. If you forget your userid, you can create a new one from the registration page.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="common-doc-group"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>-->
  <!-- 4.0 4/20 tdc: removed productname and productfamily -->
  <!-- 5.2 8/22/05 tdc:  Removed <related-list> element  -->
  <!-- ================================================================ -->
  <!-- <resource> element                                                    -->
  <!-- ================================================================ -->
  <xsd:element name="resource">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>A document resource</title>
        <desc>Reference to a resource</desc>
      </xsd:documentation>
    </xsd:annotation>
    <!-- 5.0 5/17 tdc:  Need to allow text within resource, so added mixed="true" -->
    <xsd:complexType mixed="true">
      <!-- 5.0 05/06 tdc:  For resource, updated choice from one occurence to min=1, max=unbounded -->
      <xsd:choice minOccurs="1" maxOccurs="unbounded">
        <xsd:group ref="common-doc-group"/>
      </xsd:choice>
      <xsd:attribute name="resource-category" type="category" use="required"/>
    </xsd:complexType>
  </xsd:element>
  <!-- 4.0 4/19 tdc: renamed to resource-list -->
  <!-- ================================================================ -->
  <!-- <resource-list> element                                           -->
  <!-- ================================================================ -->
  <xsd:element name="resource-list">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a list of resources</title>
        <desc>Defines a list of resources related to this article.  </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:choice maxOccurs="unbounded">
        <xsd:element ref="dl"/>
        <xsd:element ref="heading"/>
        <xsd:element ref="ol"/>
        <xsd:element ref="p"/>
        <xsd:element ref="ul"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <resources> element                                                    -->
  <!-- ================================================================ -->
  <xsd:element name="resources">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>A collection of document resources</title>
        <desc>Collection of References to  resources</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:choice>
        <xsd:element ref="resource" maxOccurs="unbounded"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <section> element                                       								-->
  <!-- ================================================================ -->
  <xsd:element name="section">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Section</title>
        <desc>Defines one section of a multi-section document, like a tutorial.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="title"/>
        <xsd:element ref="docbody"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <series> element                                            -->
  <!-- ================================================================ -->
  <!-- 5.0 3/29 tdc:  series-url now optional to accomodate older content that doesn't have it. -->
  <xsd:element name="series">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define the series title for this document</title>
        <desc>Title of the series to which the document belongs.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:sequence>
        <xsd:element ref="series-title"/>
        <xsd:element name="series-url" type="xsd:string" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <series-title> element                                            -->
  <!-- ================================================================ -->
  <xsd:element name="series-title">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define the series title for this article</title>
        <desc>Title of the series to which the document belongs.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="i"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <sidebar> element                                                -->
  <!-- ================================================================ -->
  <xsd:element name="sidebar">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a sidebar for this article</title>
        <desc>Sideline thought or concept that is not part of the general flow of the document.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="dl"/>
        <xsd:element ref="heading"/>
        <xsd:element ref="i"/>
        <xsd:element ref="ol"/>
        <xsd:element ref="p"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:element ref="ul"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
      <xsd:attribute name="width">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The width in pixels that should be used for the sidebar.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="align" type="alignment">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The horizontal alignment of the sidebar. </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- 5.1 06/06/05 JPP/EGD Added special-offers -->
  <!-- ================================================================ -->
  <!-- <special-offers> type                                                 -->
  <!-- ================================================================ -->
  <xsd:element name="special-offers">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>special-offers-section</title>
        <desc>Defines the element for the special offers section of the page</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="content-area-name" type="content-area" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The content area associated with the special offer.  To call the global special offers include or a custom special offers include, use the default value, none.  To call the special offers for a content area, select the content area.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="custom-file-name" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Provides a relative URL path for a custom special offers include: /developerworks/inc/[filename]</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <sub> (subscript) element                                        -->
  <!-- ================================================================ -->
  <xsd:element name="sub" type="xsd:string">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define subscript text</title>
        <desc>Defines text that should appear in subscript font.</desc>
      </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <subtitle> element                                               -->
  <!-- ================================================================ -->
  <xsd:element name="subtitle">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a subtitle for the article</title>
        <desc>Secondary title that may provide more information about the document, and/or contain catchy, creative wording in order to stimulate interest in the document.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="i"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <sup> (superscript) element                                      -->
  <!-- ================================================================ -->
  <xsd:element name="sup" type="xsd:string">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define superscript text</title>
        <desc>Defines text that should appear in superscript font.</desc>
      </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <system-requirements> element                                            -->
  <!-- ================================================================ -->
  <xsd:element name="system-requirements">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>The system requirements to run this content</title>
        <desc>Lists all software, hardware and operating system information required for using this content.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:group ref="common-doc-group"/>
        <!-- <xsd:element ref="common-doc"/> -->
      </xsd:choice>
      <!-- 5.2 09/20 fjc:  Added for use with podcast summary but could be used for others-->
      <xsd:attribute name="insert-std-text" type="yesno" use="optional">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Insert the standard text for system requirements</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <table> element                                                  -->
  <!-- ================================================================ -->
  <xsd:element name="table">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a table</title>
        <desc>Defines a table.  In addition to formatting tabular information, you can use tables to have more control over the layout of information.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <!-- 5.0 6/21 tdc:  Added caption -->
        <xsd:element ref="caption" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="thead" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="tr" maxOccurs="unbounded"/>
        <xsd:element ref="tfoot" minOccurs="0"/>
      </xsd:sequence>
      <xsd:attribute name="border" type="zeroOrOne">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Whether or not this table has a border.  </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="cellpadding" type="xsd:integer">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The space in pixels that is added to all sides of the cells in the table.  A value of <code>cellpadding="3"</code> means none of the cells' contents will be closer than 3 pixels to the edge of the cells.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="cellspacing" type="xsd:integer">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The space in pixels that is added between the cells in the table. </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <!-- 5.0 6/21 tdc:  Added class -->
      <xsd:attribute name="class" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The CSS class to be applied to this table</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="cols">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The dimensions of the columns in the table.  For example, the markukp <code>cols="75 75 150 50"</code> creates four columns that are 75 pixels, 75 pixels, 150 pixels, and 50 pixels wide, respectively. </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <!-- 5.0 6/21 tdc:  Added summary -->
      <xsd:attribute name="summary" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>A summary of the tables contents.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="width">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The width of the table.  You can specify this as a number of pixels (<code>width="350px"</code>) or as a percentage of the width of the page (<code>width="75%"</code>).</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- 4.0 6/3 tdc:  Split target-content into target-content-file and target-content-page -->
  <!-- ================================================================ -->
  <!-- <target-content-file> element                                                    -->
  <!-- ================================================================ -->
  <xsd:element name="target-content-file">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Target content</title>
        <desc>The characteristics of, and links to, some target document, demo, presentation, code sample, etc. (as opposed to characteristics of *this* content).</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="filename" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The filename of the content.  Use the format xxxxx.yyy (include the file extension)</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="file-type" type="file-type" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>File type of the target content.  Ex: war, jsp, pdf, zip </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="size" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The file size of the target content.  <code>78 KB</code> is an example value.  Included in alt tag for download gif. </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="target-content-type" type="content-type" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The type of the target content. Ex: technical article, demo, presentation, code sample, etc.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="file-description" type="file-description-50" use="optional">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>A short description (50 characters or less) of the target content.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="link-method-ftp" type="yesno" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines whether the ftp transport protocol is available for the licensed download.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="url-ftp" type="xsd:string" use="optional">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The ftp URL of the target file.  Must include this, and/or url-http.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="link-method-http" type="yesno" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines whether the http transport protocol is available for the licensed download.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="url-http" type="xsd:string" use="optional">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The http URL of the target file.  Must include this, and/or url-ftp.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="link-method-dd" type="yesno" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines whether the Download Director method is available for the licensed download.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="url-download-director" type="xsd:string" use="optional">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The download-director URL of the target file.  Must include this, and/or url-http, and/or url-ftp.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="show-license" type="yesno" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Whether a license page should be displayed for the target file.  All code sample downloads require a license page.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="url-license" type="xsd:string" use="optional">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The URL of the license page, if there is one, that must be displayed for the target file.  All code sample downloads require a license page.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="registration-required" type="yesno" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Whether the user must register in order to get the target file.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <!-- 4.0 6/9 tdc:  Made size optional b/c not all older files have it. -->
      <!-- 4.0 6/4 tdc:  Added urls for ftp, http, dd -->
      <!-- 4.0 6/7 tdc:  Is the xsl supposed to display some canned text if registration is required? -->
    </xsd:complexType>
  </xsd:element>
  <!-- 4.0 6/4 tdc:  Added target-content-page -->
  <!-- ================================================================ -->
  <!-- <target-content-page> element                                  -->
  <!-- ================================================================ -->
  <xsd:element name="target-content-page">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title/>
        <desc>Used to define a link to a target page that contains a download, demo, presentation, code sample, etc.. </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="target-content-type" type="content-type" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The type of the target content. Ex: white paper, article, demo, presentation, code sample, etc.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="link-text" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The keywords, each separated by a comma.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="url-target-page" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The URL for the target page.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="registration-required" type="yesno" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Whether the user must register to get the target file.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <!-- 4.0 6/6 tdc:  Added target-content-type to differentiate types, as done in target-content-file -->
      <!-- 4.0 6/7 tdc:  Is the xsl supposed to display some canned text if registration is required? -->
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <td> element                                                     -->
  <!-- ================================================================ -->
  <xsd:element name="td">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a table cell</title>
        <desc>Defines the contents of a table cell.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="dl"/>
        <xsd:element ref="font"/>
        <xsd:element ref="i"/>
        <xsd:element ref="img"/>
        <xsd:element ref="ol"/>
        <xsd:element ref="p"/>
        <xsd:element ref="pre"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:element ref="table"/>
        <xsd:element ref="ul"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
      <xsd:attribute name="align" type="alignment">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The alignment of this cell.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="bgcolor" type="xslfoColor">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The background color of this table cell.  This can be specified in two ways: as an RGB value (<code>bgcolor="#99CC33"</code>) or as one of the sixteen color values defined in the XSL-FO standard:
				<table cols="25% 25% 25% 25%">
                <tr>
                  <td>aqua</td>
                  <td>black</td>
                  <td>blue</td>
                  <td>fuchsia</td>
                </tr>
                <tr>
                  <td>gray</td>
                  <td>green</td>
                  <td>lime</td>
                  <td>maroon</td>
                </tr>
                <tr>
                  <td>navy</td>
                  <td>olive</td>
                  <td>purple</td>
                  <td>red</td>
