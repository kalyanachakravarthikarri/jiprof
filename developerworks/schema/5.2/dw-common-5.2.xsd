  <!-- ================================================================ -->
  <!-- dw-person-name     The components of a person's name 					-->
  <!-- ================================================================ -->
  <xsd:complexType name="dw-person-name">
    <xsd:sequence>
      <xsd:element name="Prefix" type="xsd:string" minOccurs="0"/>
      <xsd:element name="GivenName" type="name-type"/>
      <xsd:element name="MiddleName" type="name-type" minOccurs="0"/>
      <xsd:element name="FamilyName" type="name-type"/>
      <xsd:element name="Suffix" type="xsd:string" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ================================================================ -->
  <!-- empty - used for the empty content model                         -->
  <!-- ================================================================ -->
  <xsd:simpleType name="empty">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value=""/>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- 4.0 6/1 tdc:  Added as simpleType to enable limit of 50 char's -->
  <!-- ================================================================ -->
  <!-- file-description-50 - A short description (50 characters or less) of a target content file.     -->
  <!-- ================================================================ -->
  <xsd:simpleType name="file-description-50">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <desc>A short description (50 characters or less) of a target content file.  No formatting characters are allowed.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="50"/>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- 10/20 lk:  Added for new download-feature element -->
  <!-- 12/16 lk:  added wsil filetype per request from Jill Amaya -->
