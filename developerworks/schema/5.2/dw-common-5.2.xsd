  <!-- 5.0 6/21 tdc:  Added <th> -->
  <!-- ================================================================ -->
  <!-- <th> element                                                     -->
  <!-- ================================================================ -->
  <xsd:element name="th">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Table header cell</title>
        <desc>Defines a cell that contains header information.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
      <xsd:attribute name="align" type="alignment">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The alignment of this cell.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="colspan" type="xsd:positiveInteger">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the number of columns this cell should span. </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="class" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The CSS class to be applied to this cell.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="rowspan" type="xsd:positiveInteger">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the number of rows this cell should span.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="style" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The CSS style to be applied to this cell.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="width">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the width of this cell.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="valign">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the vertical alignment of this cell.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <thead> element                                                  -->
  <!-- ================================================================ -->
  <xsd:element name="thead">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a table header</title>
        <desc>Defines a header row for a table.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:choice maxOccurs="unbounded">
        <xsd:element ref="td"/>
      </xsd:choice>
      <xsd:attribute name="align" type="alignment">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the horizontal alignment of this cell.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="bgcolor" type="xslfoColor">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The background color of this table cell.  This can be specified in two ways: as an RGB value (<code>bgcolor="#99CC33"</code>) or as one of the sixteen color values defined in the XSL-FO standard:
				<table cols="25% 25% 25% 25%">
                <tr>
                  <td>aqua</td>
                  <td>black</td>
                  <td>blue</td>
                  <td>fuchsia</td>
                </tr>
                <tr>
                  <td>gray</td>
                  <td>green</td>
                  <td>lime</td>
                  <td>maroon</td>
                </tr>
                <tr>
                  <td>navy</td>
                  <td>olive</td>
                  <td>purple</td>
                  <td>red</td>
                </tr>
                <tr>
                  <td>silver</td>
                  <td>teal</td>
                  <td>white</td>
                  <td>yellow</td>
                </tr>
              </table>
            </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="valign" type="vertical">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the vertical alignment of the image.</desc>
