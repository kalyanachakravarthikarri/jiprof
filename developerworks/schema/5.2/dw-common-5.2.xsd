      <xsd:enumeration value="web">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The Web architecture content-area.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="webservices">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The Web services content-area.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="websphere">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The WebSphere product area.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="wireless">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The wireless content-area.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <!-- 5.2 8/23/05 tdc:  Added workplace -->
      <xsd:enumeration value="workplace">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The IBM Workplace content-area.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="xml">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The XML content-area.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- 4.0 4/29 tdc: Renamed to be more generic so that we can use it in a group of attributes.  Added enumeration values, too. -->
  <!-- ================================================================ -->
  <!-- content-type - for code or some other type of content.     -->
  <!-- ================================================================ -->
  <xsd:simpleType name="content-type">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <desc>A type of content (different from the file type)</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Code sample"/>
      <xsd:enumeration value="Demo"/>
      <!-- 5.2 09/20 fjc: add podcast type -->
      <xsd:enumeration value="Podcast"/>
      <xsd:enumeration value="Presentation"/>
      <xsd:enumeration value="Product"/>
      <xsd:enumeration value="Product documentation"/>
      <xsd:enumeration value="Specification"/>
      <xsd:enumeration value="Technical article"/>
      <xsd:enumeration value="Whitepaper"/>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- 5.0 3/11 tdc:  Added contributor here as complex-type rather than element -->
  <!-- ================================================================ -->
  <!-- contributor                                   -->
  <!-- ================================================================ -->
  <xsd:complexType name="contributor">
    <xsd:sequence>
      <xsd:element ref="bio" minOccurs="0"/>
      <xsd:element ref="img" minOccurs="0"/>
      <xsd:element name="contributor-name" type="dw-person-name"/>
      <xsd:element ref="company-name" minOccurs="0"/>
    </xsd:sequence>
    <xsd:attribute name="email" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>The email address. </desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="jobtitle" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>The contributor's job title.</desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="email-cc" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>Allows someone on the dW team, like an acquisition editor, to be copied on contributor e-mails.</desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <!-- 5.0 5/17 fjc:  Added course-type -->
  <!-- ================================================================ -->
  <!-- course-type - what type of trainig doc is this?                                                        -->
  <!-- ================================================================ -->
  <xsd:simpleType name="course-type">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <desc>Indicates the type of training the summary page is for</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="web-based">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>A web based course</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="online-instructor">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>A online instructor led course</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="classroom">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>A classroom course</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- 4.0 6/10 tdc:  Changed to choice; single digits prefixed with zero -->
