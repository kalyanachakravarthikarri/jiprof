				<table cols="25% 25% 25% 25%">
                <tr>
                  <td>aqua</td>
                  <td>black</td>
                  <td>blue</td>
                  <td>fuchsia</td>
                </tr>
                <tr>
                  <td>gray</td>
                  <td>green</td>
                  <td>lime</td>
                  <td>maroon</td>
                </tr>
                <tr>
                  <td>navy</td>
                  <td>olive</td>
                  <td>purple</td>
                  <td>red</td>
                </tr>
                <tr>
                  <td>silver</td>
                  <td>teal</td>
                  <td>white</td>
                  <td>yellow</td>
                </tr>
              </table>
            </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="valign" type="vertical">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the vertical alignment of the image.</desc>
          </xsd:documentation>
