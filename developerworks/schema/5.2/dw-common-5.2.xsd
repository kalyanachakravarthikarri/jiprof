  <!-- ================================================================ -->
  <!-- headingtype - what kind of heading is this?                      -->
  <!-- ================================================================ -->
  <!-- Can be a major or minor heading, figure heading, code heading, etc. -->
  <xsd:simpleType name="headingtype">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <desc>The classification of a particular heading.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="code">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>A heading (caption) for a code listing</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="figure">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>A heading (caption) for a figure.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="major">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>A major heading.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="minor">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>A minor heading.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="resource-list">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The heading for the list of related resources.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="sidebar">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>A heading for a sidebar.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <!-- 5.0 tdc:  Added table type -->
      <xsd:enumeration value="table">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>A heading for a table.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- ================================================================ -->
  <!-- inline - should an item be inline or a separate section?         -->
  <!-- ================================================================ -->
  <xsd:simpleType name="inline">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="inline">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>This item should be formatted inline.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="section">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>This item should be formatted as a separate section.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- 5.1 06/23/05 JPP/EGD Added page-section  -->
  <!-- ================================================================ -->
  <!-- <link>                                               -->
  <!-- ================================================================ -->
  <xsd:complexType name="link">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>link</title>
        <desc>Defines a link within a section</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="text" type="link-name"/>
      <xsd:element name="url" type="xsd:string"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- 5.1 06/23/05 JPP/EGD Added page-section  -->
  <!-- ================================================================ -->
  <!-- <link-abstract>                                               -->
  <!-- ================================================================ -->
  <xsd:complexType name="link-abstract">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>link</title>
        <desc>Defines a link with an abstract within a section</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="text" type="link-name"/>
      <xsd:element name="url" type="xsd:string"/>
      <xsd:element name="abstract-text" type="link-name"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- 5.1 06/01/05 JPP/EGD:  Added link-name -->
  <!-- ================================================================ -->
  <!-- <link-name>                                               -->
  <!-- ================================================================ -->
  <xsd:complexType name="link-name" mixed="true">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>link-name</title>
        <desc>Defines the special characters allowed within link or abstract</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:choice minOccurs="0" maxOccurs="unbounded">
      <xsd:group ref="specialCharacters"/>
    </xsd:choice>
  </xsd:complexType>
  <!-- 4.0 6/10 tdc:  Changed to enumeration; single digits get a zero prefix -->
  <!-- ================================================================ -->
  <!-- month - the month of the year -->
  <!-- ================================================================ -->
  <xsd:simpleType name="month">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01"/>
      <xsd:enumeration value="02"/>
      <xsd:enumeration value="03"/>
      <xsd:enumeration value="04"/>
      <xsd:enumeration value="05"/>
      <xsd:enumeration value="06"/>
      <xsd:enumeration value="07"/>
      <xsd:enumeration value="08"/>
      <xsd:enumeration value="09"/>
      <xsd:enumeration value="10"/>
      <xsd:enumeration value="11"/>
      <xsd:enumeration value="12"/>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- 5.0 3/11 tdc:  Added name-type -->
  <!-- ================================================================ -->
  <!-- <name-type>                                               -->
  <!-- ================================================================ -->
  <xsd:complexType name="name-type" mixed="true">
    <xsd:choice minOccurs="0" maxOccurs="unbounded">
      <xsd:element ref="a"/>
      <xsd:group ref="specialCharacters"/>
    </xsd:choice>
  </xsd:complexType>
  <!-- 5.1 06/23/05 JPP/EGD Added page-section  -->
  <!-- ================================================================ -->
  <!-- <page-section>                                               -->
  <!-- ================================================================ -->
  <xsd:complexType name="page-section">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>page-section</title>
        <desc>Defines a page section containing two or more links.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="page-section-links" type="section"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- 5.1 06/23/05 JPP/EGD Added page-section-categorized  -->
  <!-- ================================================================ -->
  <!-- <page-section-categorized>                                               -->
  <!-- ================================================================ -->
  <xsd:complexType name="page-section-categorized">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>page-section-categorized</title>
        <desc>Defines a page section that may contain category headings.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="page-section-category" type="section-category" minOccurs="1" maxOccurs="4"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- 5.1 07/21/05 JPP/EGD Added page-types  -->
  <!-- ================================================================ -->
  <!-- page-types - The type of landing page being created                -->
  <!-- ================================================================ -->
  <xsd:simpleType name="page-types">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="product">
        <xsd:annotation>
          <xsd:documentation>
            <desc>A landing page for a product</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <!-- 5.2 09/13/05 EGD added product-condensed  -->
      <xsd:enumeration value="product-condensed">
        <xsd:annotation>
          <xsd:documentation>
            <desc>A product landing page that is not frequently updated with new content so it doesn't usually have a Latest content section. It also may not contain other traditional sections like downloads and community.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="product-family">
        <xsd:annotation>
          <xsd:documentation>
            <desc>A landing page for a product family</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="integration">
        <xsd:annotation>
          <xsd:documentation>
            <desc>A landing page for product integration</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- 4.0 4/26 tdc:  No longer need origin -->
  <!-- ================================================================ -->
  <!-- paperSize - The size of paper used in a PDF file                 -->
  <!-- ================================================================ -->
  <xsd:simpleType name="paperSize">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="a4">
        <xsd:annotation>
          <xsd:documentation>
            <desc>A4-sized paper, used in most of the world.  Sheets are 210 by 297 millimeters, roughly 8.26 by 11.69 inches.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="letter">
        <xsd:annotation>
          <xsd:documentation>
            <desc>Letter-sized paper, used primarily in the United States.  Sheets are 8.5 by 11 inches, roughly 216 by 279 millimeters.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- 5.1 06/23/05 JPP/EGD Added page-section  -->
  <!-- ================================================================ -->
  <!-- <section>                                               -->
  <!-- ================================================================ -->
  <xsd:complexType name="section">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>section</title>
        <desc>Defines the links within a section</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="page-section-link" minOccurs="2" maxOccurs="unbounded" type="link"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- 5.1 06/23/05 JPP/EGD Added page-section-categorized  -->
  <!-- ================================================================ -->
  <!-- <section-category>                                               -->
  <!-- ================================================================ -->
  <xsd:complexType name="section-category">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>section-category</title>
        <desc>Defines the links within a categorized section</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="subhead" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="link-abstracts" type="link-abstract" minOccurs="2" maxOccurs="unbounded"/>
      <xsd:element name="more-link" type="xsd:string" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
